{
  "name": "react-native-expo-app",
  "version": "1.0.0",
  "main": "index.ts",
  "description": "A cross-platform mobile music player with YouTube integration, built with React Native and Expo",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export:web",
    "serve:web": "npx serve web-build",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=web-build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.1.1",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.1.26",
    "@react-navigation/stack": "^7.6.12",
    "axios": "^1.13.2",
    "expo": "~54.0.29",
    "expo-av": "^16.0.8",
    "expo-file-system": "^19.0.21",
    "expo-notifications": "~0.30.0",
    "expo-media-library": "~17.0.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-blur": "~14.0.2",
    "expo-network": "~7.0.4",
    "expo-gl": "~15.0.5",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "~15.8.0",
    "react-native-track-player": "^4.1.1",
    "react-native-web": "~0.19.13",
    "color-thief-react": "^2.1.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
